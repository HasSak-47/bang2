{
  "name": "Bangs2",
  "description" : "To use, type '!b2 {ident} {term}' to search {term} something at {ident}",
  "version": "0.1",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0"
    }
  },

  "browser_action": {
    "default_title": "bang2 entries",
    "default_popup": "popup/index.html"
  },

  "background": {
    "scripts": ["background.js"]
  },
  "permissions" : [
    "cookies",
    "storage",
    "devtools",
    "scripting",
    "activeTab"
  ],
  "omnibox": { "keyword" : "!b2" },

  "manifest_version": 2
}
