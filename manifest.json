    "gecko": {
      "strict_min_version": "58.0"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
	"permissions" : [
		"cookies", "storage"
	],
  "omnibox": { "keyword" : "!b2" },
  "manifest_version": 2
}
